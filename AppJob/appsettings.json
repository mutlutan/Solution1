{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "SelfHost": "http://127.0.0.1:5002",
    "MainConnection": "Server=.;Database=smart_bike_main;Trusted_Connection=True;Max Pool Size=500;",
    "LogConnection": "Server=.;Database=smart_bike_log;Trusted_Connection=True;Max Pool Size=500;"
  },
  "JobConfig": {
    "JobItems": [
      {
        "MethodName": "LocalWebRequest",
        "CronExpression": "*/15 * * * *" /*15 dakikada bir çalışır*/
      },
      {
        "MethodName": "SetAuditLogToDbLogFromDbMain",
        "CronExpression": "*/5 * * * *"
      },
      {
        "MethodName": "SetStatusAllVehicle",
        "CronExpression": "*/1 * * * *"
      },
      {
        "MethodName": "SendPushNotification",
        "CronExpression": "*/2 * * * *"
      },
      {
        "MethodName": "SendSmsNotification",
        "CronExpression": "*/2 * * * *"
      },
      {
        "MethodName": "SendMailWaitingNotification",
        "CronExpression": "*/1 * * * *"
      },
      {
        "MethodName": "SendMailErrorNotification",
        "CronExpression": "*/2 * * * *"
      }
    ]
  }
}
